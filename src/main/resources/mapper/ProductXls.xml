<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.WAREHOUSE.dao.ProductXlsDAO">

  <select
  	id="saveProductXls"
  	parameterType="com.WAREHOUSE.container.Product"
  	resultType="com.WAREHOUSE.container.Product"
  >
    /* ProductXls.saveProductXls */
    CALL sp_ProductXls (
      #{prodNm},
      #{prodType},
      #{houseCd},
      #{protectedQty},
      #{quality},
      #{unit},
      #{option1},
      #{option2},
      #{maker},
      #{unitPrice},
      #{compCd},
      #{remark},
      #{flagYN},
      #{issueID}
    );
  </select>

</mapper>
